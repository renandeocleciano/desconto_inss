<!DOCTYPE html>
<html>
  <head>
    <title>DescontoInss</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
    <div class="container">
      <nav class="navbar" role="navigation" aria-label="main navigation">
    <div class="navbar-brand">
    <a role="button" class="navbar-burger" aria-label="menu" aria-expanded="false" data-target="navbarBasicExample">
      <span aria-hidden="true"></span>
      <span aria-hidden="true"></span>
      <span aria-hidden="true"></span>
      <span aria-hidden="true"></span>
    </a>
  </div>

  <div id="navbarBasicExample" class="navbar-menu">
    <div class="navbar-start">
      <a class="navbar-item">
        Dashboard
      </a>
  </div>

    <div class="navbar-end">
      <div class="navbar-item">
        <div class="buttons">
          <a class="button is-primary">
            <strong>Sign up</strong>
          </a>
          <a class="button is-light">
            Log in
          </a>
        </div>
      </div>
    </div>
  </div>
</nav>
      <div class="columns">
        <div class="column is-one-quarter">
          <aside class="menu">
            <p class="menu-label">Cadastros</p>
            <ul class="menu-list">
              <li>
                <%= link_to "Faixa de Contribuicao", faixa_contribuicaos_path %>
              </li>
              <li>
                <%= link_to "Tipos de Telefones", tipo_telefones_path %>
              </li>
            </ul>
            <p class="menu-label">Proponentes</p>
            <ul class="menu-list">
              <li>
                <%= link_to "Proponentes", proponentes_path, class: "is-active" %>
              </li>
            </ul>
          </aside>
        </div>
        <div class="column is-full">
          <%= yield %>
        </div>
      </div>
    </div>
  </body>
</html>
